FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    inlet
    {
        type            totalPressure;
	p0              uniform 10;
    }

    outlet
    {
	type		fixedValue;
	value		uniform 0;
    }
}

// ************************************************************************* //
